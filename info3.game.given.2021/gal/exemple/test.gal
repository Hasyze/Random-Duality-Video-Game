Joueur1(Init){
  * (Init)
  | Key(z) & Key(q) & !(Cell(NW,O) / Cell(NW,J)) ? Move(NW) :(Action)
  | Key(z) & Key(d) & !(Cell(NE,O) / Cell(NE,J)) ? Move(NE) :(Action)
  | Key(s) & Key(q) & !(Cell(SW,O) / Cell(SW,J)) ? Move(SW) :(Action)
  | Key(s) & Key(d) & !(Cell(SE,O) / Cell(SE,J)) ? Move(SE) :(Action)
  | Key(z) & !(Cell(N,O) / Cell(N,J)) ? Move(N) :(Action)
  | Key(q) & !(Cell(W,O) / Cell(W,J)) ? Move(W) :(Action)
  | Key(s) & !(Cell(S,O) / Cell(S,J)) ? Move(S) :(Action)
  | Key(d) & !(Cell(E,O) / Cell(E,J)) ? Move(E) :(Action)
  | Key(SPACE) ? Hit() :(Init)
  * (Action)
  | Key(SPACE) ? Hit() :(Init)
  | True ?  :(Init)
}

Joueur2(Init){
  * (Init)
  | Key(FU) & Key(FR) & !(Cell(NW,O) / Cell(NW,J)) ? Move(NE) :(Init)
  | Key(FU) & Key(FL) & !(Cell(NE,O) / Cell(NE,J)) ? Move(NW) :(Init)
  | Key(FD) & Key(FR) & !(Cell(SW,O) / Cell(SW,J)) ? Move(SE) :(Init)
  | Key(FD) & Key(FL) & !(Cell(SE,O) / Cell(SE,J)) ? Move(SW) :(Init)
  | Key(FU) & !(Cell(N,J) / Cell(N,O)) ? Move(N) :(Init)
  | Key(FL) & !(Cell(W,J) / Cell(W,O)) ? Move(W) :(Init)
  | Key(FD) & !(Cell(S,J) / Cell(S,O)) ? Move(S) :(Init)
  | Key(FR) & !(Cell(E,J) / Cell(E,O)) ? Move(E) :(Init)
  | Key(ENTER) ? Hit() :(Init)
}

Con(Init){
 * (Init)
  | True ? Wait :(Init)
}

Projectilesimple(Init){
 * (Init)
  | True ? Move(N) :(Init)
}

